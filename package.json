{
  "name": "sistema-ia-conversacional-avancada",
  "version": "1.0.0",
  "description": "Sistema de IA Conversacional com Auto-Aprendizado e MemÃ³ria Persistente",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npx next dev -p 3000",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "npm run start:system",
    "start:system": "./scripts/start_system.sh",
    "stop:system": "./scripts/stop_system.sh",
    "monitor": "./scripts/monitor_system.sh",
    "setup": "npm run setup:all",
    "setup:all": "npm run setup:prerequisites && npm run setup:backend && npm run setup:frontend",
    "setup:prerequisites": "./scripts/install_prerequisites.sh",
    "setup:backend": "./scripts/setup_backend.sh",
    "setup:frontend": "./scripts/setup_frontend.sh",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && python -m pytest",
    "test:frontend": "cd frontend && npm run test"
  },
  "keywords": [
    "IA",
    "ChatBot",
    "OpenManus",
    "Machine Learning",
    "Auto-Aprendizado",
    "Conversacional",
    "TeenyTinyLlama"
  ],
  "author": "Sistema IA Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}